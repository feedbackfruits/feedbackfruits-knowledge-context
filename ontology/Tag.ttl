:Tag
  a owl:Class ;
  rdfs:subClassOf owl:Thing, oa:SemanticTag .

:tagOf
  a owl:ObjectProperty ;
  rdfs:domain :Tag ;
  rdfs:range :Taggable ;
  owl:inverseOf :tag .

:about
  a owl:ObjectProperty ;
  rdfs:subPropertyOf schema:about ;
  rdfs:domain :Tag ;
  rdfs:range :Entity ;
  owl:inverseOf :subjectOf .

:score
  a owl:ObjectProperty, owl:FunctionalProperty ;
  rdfs:domain :Tag ;
  rdfs:range xsd:decimal .
