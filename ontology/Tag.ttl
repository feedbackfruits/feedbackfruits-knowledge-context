:Tag
  a owl:Class ;
  rdfs:subClassOf oa:SemanticTag .

:resource
  a owl:DatatypeProperty ;
  rdfs:domain :Tag ;
  rdfs:range :Resource ;
  owl:inverseOf :tag .

:entity
  a owl:DatatypeProperty ;
  rdfs:domain :Tag ;
  rdfs:range :Entity ;
  owl:inverseOf :tag .

:score
  a owl:ObjectProperty, owl:FunctionalProperty ;
  rdfs:domain :Tag ;
  rdfs:range xsd:decimal .
