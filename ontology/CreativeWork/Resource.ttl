:Resource
  a owl:Class ;
  rdfs:subClassOf schema:CreativeWork .

:topic
  a owl:ObjectProperty ;
  rdfs:domain :Resource ;
  rdfs:range :Topic ;
  owl:inverseOf :resource .

:tag
  a owl:DatatypeProperty ;
  rdfs:domain :Resource ;
  rdfs:range :Tag ;
  owl:inverseOf :resource .

:annotation
  a owl:DatatypeProperty ;
  rdfs:subPropertyOf :tag ;
  rdfs:domain :Resource ;
  rdfs:range :Annotation ;
  owl:inverseOf :resource .
