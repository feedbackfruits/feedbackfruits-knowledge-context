:Resource
  a owl:Class ;
  rdfs:subClassOf schema:CreativeWork .

:topic
  a owl:ObjectProperty ;
  rdfs:domain :Resource ;
  rdfs:range :Topic ;
  owl:inverseOf :resource .

:about
  a owl:ObjectProperty ;
  rdfs:subPropertyOf schema:about ;
  rdfs:domain :Resource ;
  rdfs:range :Entity ;
  owl:inverseOf :subjectOf .
