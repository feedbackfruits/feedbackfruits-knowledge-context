@prefix : <https://knowledge.express/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .


:CreativeWork
  a owl:Class ;
  rdfs:subClassOf owl:Thing ;
  rdfs:label "CreativeWork"@en ;
  rdfs:comment "A CreativeWork about entities related to topics."@en .

schema:name
  a owl:ObjectProperty, owl:FunctionalProperty ;
  rdfs:domain :CreativeWork ;
  rdfs:range xsd:string .

schema:description
  a owl:ObjectProperty, owl:FunctionalProperty ;
  rdfs:domain :CreativeWork ;
  rdfs:range xsd:string .

schema:image
  a owl:ObjectProperty ;
  rdfs:domain :CreativeWork ;
  rdfs:range xsd:string .

schema:license
  a owl:ObjectProperty ;
  rdfs:domain :CreativeWork ;
  rdfs:range xsd:string .

schema:sourceOrganization
  a owl:ObjectProperty ;
  rdfs:domain :CreativeWork ;
  rdfs:range xsd:string .

schema:text
  a owl:ObjectProperty ;
  rdfs:domain :CreativeWork ;
  rdfs:range xsd:string .

schema:about
  a owl:ObjectProperty ;
  rdfs:label "CreativeWorks on"@en ;
  rdfs:comment "The topic of the discussion the CreativeWork belongs to."@en ;
  rdfs:domain :CreativeWork ;
  rdfs:range :Entity ;
  owl:inverseOf schema:subjectOf .
